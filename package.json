{
	"name": "voice-collab",
	"displayName": "Voice Collab",
	"description": "Let's harness the power of voice.",
	"version": "0.0.1",
	"publisher": "sandy_codes_py",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:voice.notification",
		"onCommand:voice.activate_voice",
		"onCommand:voice.deactivate_voice"
	],
	"main": "./out/extension.js",
	"repository": "https://github.com/Santhoshkumard11/Voice-Collab",
	"icon": "media/icon.png",
	"contributes": {
		"commands": [
			{
				"command": "voice.notification",
				"title": "Voice Collab: Notification"
			},
			{
				"command": "voice.activate_voice",
				"title": "Voice Collab: Activate"
			},
			{
				"command": "voice.deactivate_voice",
				"title": "Voice Collab: Deactivate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"@deepgram/sdk": "^1.2.4",
		"extendable-media-recorder": "^6.6.2",
		"ws": "^7.5.7"
	}
}
